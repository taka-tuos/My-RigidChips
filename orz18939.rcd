Val{
	ART(max=6,step=1)
	BRAKE(max=128,step=64)
	ENGINE(min=-8192,max=20000,step=2048)
	L(min=-30,max=38,step=4)
	R(min=-38,max=30,step=4)
	HH(min=-15,max=15,step=3)
}
Key{
	0:ENGINE(step=8192)
	1:ENGINE(step=-2048)
	2:L(step=-4),R(step=-4),ART(step=1),HH(step=3)
	3:L(step=4),R(step=4),ART(step=1),HH(step=-3)
	4:BRAKE(step=64)
}
Body{
	Core(){
		E:Weight(){
			E:Frame(Angle=-91){
				W:Cowl(Angle=-100){
					W:Cowl(Angle=-100){
						W:Cowl(Color=#FF8000,Angle=-70){
							N:Cowl(Color=#FF8000){}
							S:Cowl(Color=#FF8000){}
							W:Cowl(Color=#FF8000,Angle=-90){
								S:Cowl(Color=#FF8000){}
								N:Cowl(Color=#FF8000){}
								W:Cowl(Color=#000000,Angle=-75){
									S:Cowl(Color=#000000){}
									N:Cowl(Color=#000000){}
									E:Cowl(Angle=-100){
										E:Cowl(Color=#000000,Angle=-2.5){
											S:Cowl(Color=#FF8000,Angle=-30,Option=3){}
											N:Cowl(Color=#FF8000,Angle=-30,Option=4){}
											E:Cowl(Color=#FF8000,Angle=-62.85){
												N:Cowl(Color=#000000,Angle=-50){
													N:Cowl(Color=#FF8000,Angle=45){
														N:Cowl(Color=#FF8000,Angle=-10){
															N:Cowl(Color=#FF8000,Angle=-30){
																W:Cowl(Color=#FFFF00,Option=4){}
															}
														}
													}
												}
												S:Cowl(Color=#000000,Angle=-50){
