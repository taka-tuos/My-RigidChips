													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
				S:TrimF(){
					S:Wheel(Angle=-ART,Power=-ENGINE,Brake=BRAKE,Option=1,Effect=4){}
				}
				N:TrimF(){
					N:Wheel(Angle=L,Power=-ENGINE,Brake=BRAKE,Option=1,Effect=4){}
				}
			}
		}
		W:Weight(){
			W:Frame(Angle=-91){
				E:Cowl(Angle=-100){
					E:Cowl(Angle=-100){
						E:Cowl(Color=#FF8000,Angle=-70){
							N:Cowl(Color=#FF8000){}
							S:Cowl(Color=#FF8000){}
							E:Cowl(Color=#FF8000,Angle=-90){
								N:Cowl(Color=#FF8000){}
								S:Cowl(Color=#FF8000){}
								E:Cowl(Color=#000000,Angle=-75){
									N:Cowl(Color=#000000){}
									S:Cowl(Color=#000000){}
									W:Cowl(Angle=-100){
										W:Cowl(Color=#000000,Angle=-2.5){
											S:Cowl(Color=#FF8000,Angle=-30,Option=4){}
											N:Cowl(Color=#FF8000,Angle=-30,Option=3){}
											W:Cowl(Color=#FF8000,Angle=-62.5){
												N:Cowl(Color=#000000,Angle=-50){
													N:Cowl(Color=#FF8000,Angle=45){
														N:Cowl(Color=#FF8000,Angle=-10){
															N:Cowl(Color=#FF8000,Angle=-30){
																E:Cowl(Color=#FFFF00,Option=3){}
															}
