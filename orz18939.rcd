					W:Chip(){
						N:Chip(Angle=10){}
					}
				}
			}
			S:TrimF(Angle=90,Option=1){
				N:RudderF(Angle=90,Option=1){
					W:Chip(Angle=-HH){}
				}
				S:Frame(Option=1){
					S:Rudder(Angle=-90){}
				}
			}
