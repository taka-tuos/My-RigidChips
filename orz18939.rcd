													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
				N:TrimF(){
					N:Wheel(Angle=-R,Power=ENGINE,Brake=BRAKE,Option=1,Effect=4){}
				}
				S:TrimF(){
					S:Wheel(Angle=-ART,Power=ENGINE,Brake=BRAKE,Option=1,Effect=4){}
				}
			}
		}
		N:Weight(){
			E:Trim(Angle=-10){}
			W:Trim(Angle=10){}
