_wsize_hlf = 160

function Rotete2D(x1,y1,a)
	return x1 * math.cos(a) - y1 * math.sin(a), x1 * math.sin(a) + y1 * math.cos(a)
end

function OnDraw_Window_Radar(obj)
	XGUI.SetWindowPosition(obj,0,32+200+20)
	
