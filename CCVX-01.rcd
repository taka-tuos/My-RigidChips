	local v=_LINER(V,_KEY(1)*16+_KEY(0)*-16,8)
	
	if _md == 1 then
		p=VEL*50000
	end
	
	if _md == 2 then
		p=VEL*1000000
	end
	
	P=p
	
	if p < 0 then
		WX=-WX
		WY=-WY
	end
	
	local _r,_v = 0,0
	
	if _KEY(2) == 0 and _KEY(3) == 0 then
		_r=WZ*32
		JZ=WZ*-600000
	else
