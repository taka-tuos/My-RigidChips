	XGUI.Move2D(_wsize_hlf,_wsize_hlf)
	XGUI.Line2D(_wsize_hlf+math.sin(math.rad(_ZOOM(0)))*_wsize_hlf,0)
	
	XGUI.Move2D(_wsize_hlf,_wsize_hlf)
	XGUI.Line2D(_wsize_hlf,0)
	
	local i
	
	
