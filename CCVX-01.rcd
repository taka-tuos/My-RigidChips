			XGUI.DrawVectorStringCenter("Å~")
			XGUI.SetStringPosition(x2+6,y2+6)
			XGUI.DrawVectorString(_PLAYERNAME(i))
		end
	end
	
	XGUI.WindowDrawEnd()
end

function OnDraw_Window_Vert(obj)
	XGUI.SetWindowPosition(obj,320,32+200+20)
	
	XGUI.WindowDrawBegin(obj)
	
	local i
	
	for i=0,35 do
		local x,y = Rotete2D(-_wsize_hlf,0,math.rad(i*10))
		XGUI.Move2D(x+_wsize_hlf,y+_wsize_hlf)
		XGUI.Line2D(x*0.9+_wsize_hlf,y*0.9+_wsize_hlf)
		
		XGUI.SetStringSize(12)
		XGUI.SetStringPosition(x*0.8+_wsize_hlf,y*0.8+_wsize_hlf)
		
		local xa = -math.asin(_ZY())
		
		local x,y = Rotete2D(-_wsize_hlf,0,xa)
		
		XGUI.Move2D(x*0.8+_wsize_hlf,y*0.8+_wsize_hlf)
		XGUI.Line2D(-x*0.8+_wsize_hlf,-y*0.8+_wsize_hlf)
		
		local x1,y1 = Rotete2D(0,16,xa)
		
		XGUI.Move2D(_wsize_hlf+x1,_wsize_hlf+y1)
		XGUI.Line2D(-x*0.8+_wsize_hlf,-y*0.8+_wsize_hlf)
		
		XGUI.Move2D(_wsize_hlf-x1,_wsize_hlf-y1)
		XGUI.Line2D(-x*0.8+_wsize_hlf,-y*0.8+_wsize_hlf)
		
		XGUI.Move2D(_wsize_hlf+x1,_wsize_hlf+y1)
		XGUI.Line2D(x*0.8+_wsize_hlf,y*0.8+_wsize_hlf)
		
		XGUI.Move2D(_wsize_hlf-x1,_wsize_hlf-y1)
		XGUI.Line2D(x*0.8+_wsize_hlf,y*0.8+_wsize_hlf)
		
		XGUI.Move2D(_wsize_hlf-x1,_wsize_hlf-y1)
		XGUI.Line2D(_wsize_hlf+x1,_wsize_hlf+y1)
		
		local j = i
		
		if i > 9 then j = 18 - i end
		
		if i > 27 then j = -9 + (i - 27) end
		
		XGUI.DrawVectorStringCenter(string.format("%d",math.abs(j*10)))
	end
	
	XGUI.WindowDrawEnd()
end
