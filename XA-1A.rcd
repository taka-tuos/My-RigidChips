function _NORMAL3(a,b,c,d,e,f)
	return a / _LEN3(a,b,c), b / _LEN3(a,b,c), c / _LEN3(a,b,c), d, e, f
end


function RaySphereAAM(lx, ly, lz, vx, vy, vz, px, py, pz, r)
	px = px - lx
	py = py - ly
	pz = pz - lz

	local A = vx * vx + vy * vy + vz * vz
	local B = vx * px + vy * py + vz * pz
	local C = px * px + py * py + pz * pz - r * r

	if A == 0 then
		return false -- ƒŒƒC‚Ì’·‚³‚ª0
	end

	local s = B * B - A * C

	if s < 0 then
		return false -- Õ“Ë‚µ‚Ä‚¢‚È‚¢
	end

	s = math.sqrt(s);
	local a1 = ( B - s ) / A
	local a2 = ( B + s ) / A

	if a1 < 0 and a2 < 0 then
		return false -- ƒŒƒC‚Ì”½‘Î‚ÅÕ“Ë
	end

	local q1x = lx + a1 * vx
	local q1y = ly + a1 * vy
	local q1z = lz + a1 * vz
	local q2x = lx + a2 * vx
	local q2y = ly + a2 * vy
	local q2z = lz + a2 * vz

	return true,q1x,q1y,q1z,q2x,q2y,q2z
end

