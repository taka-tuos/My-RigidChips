	__agc_ntp[2+__agc_next] = _ntp
	
	if _KEYDOWN(4) == 1 then
		local agsc1=_G['AGSC'..(__agc_next+1)]
		local agsc2=_G['AGSC'..(__agc_next+2)]
		
