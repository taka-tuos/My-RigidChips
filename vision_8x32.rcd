--dofile("vision_font.lua")
require("vision_font_8.lua")
require("vision_font_8j.lua")

__GL_COLOR = 16777215

function _GL_CLEAR()
	local x,y
	for x=0,31 do
		for y=0,7 do
			_G[string.format("X%02dY%02d",x,y)] = 0
		end
	end
end

function _GL_POINT(x,y,c)
	if x>=0 and x<32 and y>=0 and y<8 then _G[string.format("X%02dY%02d",math.floor(x),math.floor(y))] = c end
end

function _GL_DRAWC(x,y,c)
	local vx,vy
	for vx=0,7 do
		for vy=0,7 do
			if string.byte(dotfont_dat[c],vy*8+vx+1) == string.byte('#') then
				_GL_POINT(vx+x,vy+y,__GL_COLOR)
			end
		end
	end
end

function _GL_DRAWC_J(x,y,c)
	local vx,vy
	for vx=0,7 do
		for vy=0,7 do
			if string.byte(dotfontj_dat[c],vy*8+vx+1) == string.byte('#') then
				_GL_POINT(vx+x,vy+y,__GL_COLOR)
			end
		end
	end
end

function _GL_COLOR(c)
	__GL_COLOR = c
end


function _GL_DRAWS(x,y,s)
	local i,j
	local lb = 0
	j = 0
	for i = 1,string.len(s) do
		local ji = string.byte(s,i)
		if lb == 0 then
			if (129 <= ji and ji<=159) or (224 <= ji and ji <= 252) then
				lb = ji
			else
				_GL_DRAWC(x+j,y,ji)
			end
		else
			local k,t = 0,0
			
			if 129 <= lb and lb <= 159 then
				k = (lb - 129) * 2
			else
				k = (lb - 224) * 2 + 62
			end
			
			if 64 <= ji and ji <= 126 then
				t = ji - 64
			elseif 128 <= ji and ji <= 158 then
				t = ji - 128 + 63
			else
				t = ji - 159
				k = k + 1
			end
			_GL_DRAWC_J(x+j-4,y,(k * 94 + t))
			lb = 0
		end
		j=j+4
	end
end

_x = 32
_c = 255*256

__stringnum = 1

__strings = {
	{		90,		"あけましておめでとう　　　　祝☆2017　　　　今年もRC頑張ろう　　　　【Ads by sabatotan】初詣は挫折神社へ"	, 1, 0},
	{		-1,		""						, 0, 0},
}

__sp = 1
__ed = 1
__tk = 0

function OnFrame()
	_GL_CLEAR()
	local s = __strings[__sp][2]
	if __sp <= __stringnum then
		if __strings[__sp+1][1] == _TICKS() then __sp = __sp + 1 __ed = 1 end
		if __ed == 1 then
			_GL_DRAWS(_x,1,s)
			if __strings[__sp][3] == 1 then
				_x = _x - 1
				if _x < -string.len(s)*4 then _x = string.len(s)*4 if __strings[__sp][4] == 1 then __ed = 0 end end
			else 
				_x = 0
			end
		end
	else
		
	end
	
end
